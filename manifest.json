{  
    "manifest_version": 2,
    "name": "Spot Wiki Edit",
    "version": "1.1.0",
  
    "description": "Spot Wiki Edit",

    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": [
          "SpotWikiEdit.js"
        ]
      }
    ],

    "options_ui": {
        "page": "options/options.html",
        "browser_style": true,
        "chrome_style": true
      },
  
    "background": {
      "scripts": [
        "SpotWikiEdit.js"
      ]
    }
  }
  
  